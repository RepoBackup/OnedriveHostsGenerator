{
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/handler.rs"
    }
  ],
  "builds": [
    {
      "src": "api/handler.rs",
      "use": "vercel-rust",
      "config": {
        "functions": {
          "api/handler.rs": {
            "runtime": "vercel-rust@4.0.2"
          }
        }
      }
    }
  ]
}